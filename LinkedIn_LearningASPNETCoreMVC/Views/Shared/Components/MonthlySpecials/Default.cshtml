@model IEnumerable<ExploreCalifornia.Models.MonthlySpecial>

<h1>Monthly Specials</h1>

@foreach(var special in Model)
{
    <h2 class="top">
        <image src="/images/@(special.Key)_bug.gif" alt="@special.Name" width="75" height ="75"></image>
        @special.Name
    </h2>
    <p>
        @special.Type<br />
        <a href="/tours/@(special.Key).htm">$@special.Price</a>
    </p>
}
